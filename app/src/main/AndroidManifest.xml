<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akshay.oneexamadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneExamAdmin">
        <activity
            android:name=".SplashActivity2"
            android:exported="true"
            android:theme="@style/SplashStyle"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" android:theme="@style/SplashStyle"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".NewExamActivity"
            android:windowSoftInputMode="adjustPan"
            />
        <activity android:name=".ExamDetailActivity" />
        <activity android:name=".ExamAttemptsActivity" />
        <activity android:name=".ProfileActivity" />
    </application>

</manifest>